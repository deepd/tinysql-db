CREATE TABLE course (sid INT, homework INT, project INT, exam INT, grade STR20)
INSERT INTO course (sid, homework, project, exam, grade) VALUES (1, 99, 100, 100, "A")
INSERT INTO course (sid, homework, project, exam, grade) VALUES (2, 100, 100, 98, "C")
INSERT INTO course (sid, homework, project, exam, grade) VALUES (3, 100, 69, 64, "C")
CREATE TABLE course2 (sid INT, exam INT, grade STR20)
INSERT INTO course2 (sid, exam, grade) VALUES (11, 100, "A")
INSERT INTO course2 (sid, exam, grade) VALUES (12, 25, "E")
INSERT INTO course2 (sid, exam, grade) VALUES (13, 0, "A")
INSERT INTO course2 (sid, exam, grade) VALUES (2, 99, "B")
INSERT INTO course2 (sid, exam, grade) VALUES (3, 98, "C")
INSERT INTO course2 (sid, exam, grade) VALUES (4, 97, "D")
INSERT INTO course2 (sid, exam, grade) VALUES (5, 66, "A")
INSERT INTO course2 (sid, exam, grade) VALUES (6, 65, "B")
CREATE TABLE course3 (sid INT, homework INT, project INT, exam INT, grade STR20)
INSERT INTO course3 (sid, homework, project, exam, grade) VALUES (1, 99, 100, 100, "A")
INSERT INTO course3 (sid, homework, project, exam, grade) VALUES (2, 100, 100, 98, "C")
INSERT INTO course3 (sid, homework, project, exam, grade) VALUES (3, 100, 69, 64, "C")
SELECT course.grade FROM course, course2, course3 WHERE course.grade = course2.grade AND course.sid = course3.sid
exit
